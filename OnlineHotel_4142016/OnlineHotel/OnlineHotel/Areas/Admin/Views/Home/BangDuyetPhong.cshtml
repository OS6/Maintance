@model List<OnlineHotel.Models.BANGDUYETPHONG>
@{
    ViewBag.Title = "BangDuyetPhong";
    Layout = "~/Areas/Admin/Views/Shared/_Layouts.cshtml";
}

@section header{
   duyet phong
}
<div class="col-lg-12">
    <div>
        <div class="panel panel-default">
            <div class="panel-heading">
                Danh sách đặt
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Loại phòng</th>
                                <th>Ngày bắt đầu thuê</th>
                                <th>Ngày kết thúc thuê</th>
                                <th>#</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr id="row">
                                    <td>@item.MaLP</td>
                                    <td>@item.NgayBatDauThue</td>
                                    <td>@item.NgayKetThucThue</td>
                                </tr>

                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
        </div>
    </div>
</div>

